{
  "manifest_version": 3,
  "name": "SV-Meldeportal JLohn Autofill",
  "version": "0.0.0",
  "description": "Fills SV-Meldeportal contribution fields from a JLohn export line (popup + optional clipboard).",
  "permissions": ["scripting", "activeTab", "clipboardRead"],
  "host_permissions": ["*://*.sv-meldeportal.de/*", "*://sv-meldeportal.de/*"],
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png"
  },
  "action": {
    "default_title": "SV-Meldeportal aus JLohn bef√ºllen",
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png",
      "48": "icons/icon48.png"
    }
  }
}

